#include <iostream>
using namespace std;

int main(int argc,char** argv){
	int a,b,n,n1;
	
	cout << "Integer1:";
	cin  >> a;
	cout << "Integer2:";
	cin  >> b;
	
	cout << "Result1\n";
	
	n = 0; /*n表有幾個*/
	for (int i=1; i<=a ; i++){
		if (i%7==0 && i%3!=0){
			cout << i << endl;
			n = n + 1;	
		}
	}
	cout << n << endl;
	
	cout << "Result2\n";
	
	n1 = 0; /*n表有幾個*/
	for (int i=1; i<=b ; i++){
		if (i%7==0 && i%3!=0){
			cout << i << endl;
			n1 = n1 + 1;	
		}
	}
	cout << n1 ;
	
	
	return 0;
} 

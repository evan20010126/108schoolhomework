#include <iostream>
using namespace std;

int main (int argc,char** argv){
	int a,b;
	cout << "Integer1:";
	cin  >> a;
	cout << "Integer2:";
	cin  >> b;
	cout << "Result1" << endl;
	
	for (int i=1; i<=a ; i++){
		if (a%i==0){
			cout << i << endl;
		}
	}
	
	for (int i=2; i<a ; i++){
		if (a%i==0){
			break;
		}
		cout << "Prime" << endl;
		break;
	}
	
	cout << "Result2" << endl;
	
	for (int i=1; i<=b ; i++){
		if (b%i==0){
			cout << i ;
		}
		
		if (b%i==0 && b!=i){
			cout << endl;
		}
	}
	
	for (int i=2; i<b ; i++){
		if (b%i==0){
			break;
		}
		cout << "\nPrime" ;
		break;
	}
	
	return 0;
} 

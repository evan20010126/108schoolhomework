#include <iostream>
using namespace std;

int main(int argc,char** argv){
	int n,m,num,ans=0,n1,m1,ans1=0;
	cout << "N:";
	cin  >> n;
	cout << "M:";
	cin  >> m;
	cout << "Result:";
	for (int i=1; (n/i)>0; i=i*10){
		num = n/i%10;
		if (num ==m){
			ans = ans + 1; 
		}
	}
	cout << ans << endl;
	
	cout << "N:";
	cin  >> n1;
	cout << "M:";
	cin  >> m1;
	cout << "Result:";
	for (int i=1; (n1/i)>0; i=i*10){
		num = n1/i%10;
		if (num ==m1){
			ans1 = ans1 + 1; 
		}
	}
	cout << ans1;
	return 0;
} 

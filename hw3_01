#include <iostream>
using namespace std;

int main(int argc, char** argv)
{
	int a,b,c;
	cout << "Integer1:";
	cin  >> a;
	cout << "Integer2:";
	cin  >> b;
	cout << "Integer3:";
	cin  >> c;
	if (a%3==0 && a%5==0 && a%7!=0)
	{
		cout << "Result1:0\n";
	}else{
		cout << "Result1:1\n";
	}
	
	if (b%3==0 && b%5==0 && b%7!=0)
	{
		cout << "Result2:0\n";
	}else{
		cout << "Result2:1\n";
	}
	
	if (c%3==0 && c%5==0 && c%7!=0)
	{
		cout << "Result3:0";
	}else{
		cout << "Result3:1";
	}
	return 0;
} 

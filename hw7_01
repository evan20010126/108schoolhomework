#include <iostream>
using namespace std;

void solidsquare(int side,char c){
	for (int i=1; i<=side ;i++){
		for (int r=1; r<=side;r++){
			cout << c;
		}
		if (i!=side){
			cout << endl;
		}
	}
}



int main(int argc,char** argv){
	int N,N1;
	char C,C1;
	cout << "Integer1:";
	cin  >> N;
	cout << "Char1:";
	cin  >> C;

	cout << "Integer2:";
	cin  >> N1;
	cout << "Char2:";
	cin  >> C1;
	cout << "Result1:\n";
	solidsquare(N,C);
	cout << "\nResult2:\n";
	solidsquare(N1,C1);
	
	return 0;
}

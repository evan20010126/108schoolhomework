#include <iostream>
using namespace std;

int main(int argc,char** argv){
	int a,b;
	float ans1,ans2;
	cout << "Input1:" ;
	cin  >> a;
	cout << "Input2:" ;
	cin  >> b;
	
	ans1 = (5.0/9.0)*(a-32);
	ans2 = (5.0/9.0)*(b-32);
	cout << "Result1:" << ans1 << endl;
	cout << "Result2:" << ans2;
	return 0;
} 

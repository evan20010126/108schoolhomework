		total = total + 4000;
	}else if (d%10000 == my1%10000 || e%10000 == my1%10000 || f%10000 == my1%10000)
	{
		total = total + 1000;
	}else if (d%1000 == my1%1000 || e%1000 == my1%1000 || f%1000 == my1%1000)
	{
		total = total + 200;
	}
	
	if (d == my2 || e == my2 || f ==my2)
